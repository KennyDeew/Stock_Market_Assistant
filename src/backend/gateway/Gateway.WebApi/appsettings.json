{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "FRONTEND_ORIGIN": "http://localhost:5273",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v1/auth/{**catch-all}"
        }
      },
      "auth-account-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/v1/account/{**catch-all}"
        }
      },
      "portfolio-route": {
        "ClusterId": "portfolio-cluster",
        "Match": {
          "Path": "/api/v1/portfolios/{**catch-all}"
        }
      },
      "portfolio-assets-route": {
        "ClusterId": "portfolio-cluster",
        "Match": {
          "Path": "/api/v1/portfolio-assets/{**catch-all}"
        }
      },
      "portfolio-alerts-route": {
        "ClusterId": "portfolio-cluster",
        "Match": {
          "Path": "/api/v1/alerts/{**catch-all}"
        }
      },
      "stockcard-share-route": {
        "ClusterId": "stockcard-cluster",
        "Match": {
          "Path": "/api/v1/ShareCard/{**catch-all}"
        }
      },
      "stockcard-bond-route": {
        "ClusterId": "stockcard-cluster",
        "Match": {
          "Path": "/api/v1/BondCard/{**catch-all}"
        }
      },
      "stockcard-crypto-route": {
        "ClusterId": "stockcard-cluster",
        "Match": {
          "Path": "/api/v1/CryptoCard/{**catch-all}"
        }
      },
      "stockcard-pricehub-route": {
        "ClusterId": "stockcard-cluster",
        "Match": {
          "Path": "/pricehub/{**catch-all}"
        }
      },
      "analytics-route": {
        "ClusterId": "analytics-cluster",
        "Match": {
          "Path": "/api/analytics/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://authservice-api:8080"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:10",
            "Timeout": "00:00:05",
            "Path": "/health"
          }
        }
      },
      "portfolio-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://portfolioservice-api:8080"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:10",
            "Timeout": "00:00:05",
            "Path": "/health"
          }
        }
      },
      "stockcard-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://stockcardservice-api:8080"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:10",
            "Timeout": "00:00:05",
            "Path": "/health"
          }
        },
        "HttpClient": {
          "RequestHeaderEncoding": "Latin1"
        }
      },
      "analytics-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://analyticsservice-api:8080"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:10",
            "Timeout": "00:00:05",
            "Path": "/health"
          }
        }
      }
    }
  }
}
