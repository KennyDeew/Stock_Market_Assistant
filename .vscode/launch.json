{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "ðŸš€ Start Docker Compose (All Services)",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/start-docker-compose.ps1",
			"args": [
				"-Build"
			],
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true,
				"clear": true
			}
		},
		{
			"name": "ðŸš€ Start Docker Compose (Detached)",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/start-docker-compose.ps1",
			"args": [
				"-Build",
				"-Detached"
			],
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		},
		{
			"name": "Test Portfolio Asset Creation",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/test_portfolio_asset_creation.ps1",
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Remove All Docker Containers",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/remove_all_docker_containers.ps1",
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "ðŸ“‹ View Error Logs",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/view_error_logs.ps1",
			"args": [
				"-SaveToFile"
			],
			"cwd": "${workspaceFolder}",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true,
				"clear": true
			}
		},
		{
			"name": "ðŸ“„ Open Latest Log",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/open_latest_log.ps1",
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "ðŸ”§ Fix Docker Build",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/fix_docker_build.ps1",
			"args": [],
			"cwd": "${workspaceFolder}",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true,
				"clear": true
			}
		},
		{
			"name": "ðŸ”§ Fix PowerShell Extension",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/fix_powershell_extension.ps1",
			"args": [],
			"cwd": "${workspaceFolder}",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true,
				"clear": true
			}
		},
		{
			"name": "Convert MMD to SVG",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/Presentation/convert_mmd_to_svg.py",
			"args": [
				"${file}"
			],
			"console": "integratedTerminal",
			"justMyCode": true,
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Convert MMD to DOCX",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/Presentation/convert_mmd_to_docx.py",
			"args": [
				"${file}"
			],
			"console": "integratedTerminal",
			"justMyCode": true,
			"cwd": "${workspaceFolder}"
		},
		{
			// Use IntelliSense to find out which attributes exist for C# debugging
			// Use hover for the description of the existing attributes
			// For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
			"name": ".NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			// If you have changed target frameworks, make sure to update the program path.
			"program": "${workspaceFolder}/src/backend/services/AnalyticsService/AnalyticsService.WebApi/bin/Debug/net9.0/AnalyticsService.WebApi.dll",
			"args": [],
			"cwd": "${workspaceFolder}/src/backend/services/AnalyticsService/AnalyticsService.WebApi",
			"stopAtEntry": false,
			// Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
			"serverReadyAction": {
				"action": "openExternally",
				"pattern": "\\bNow listening on:\\s+(https?://\\S+)"
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Views"
			}
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach"
		}
	]
}